@model WebApplicationCasusWasmachine.Models.Device

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>Device</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Brand)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Brand)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Model)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Model)
        </dd>
        <dt class = "col-sm-2">
            Energy label
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.energyLabel)
        </dd>
        <dt class = "col-sm-2">
            Kilowatt-Hour (KwH)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.KwH)
        </dd>
        <dt class = "col-sm-2">
            Manufacture Date
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.manufactureDate)
        </dd>
        <dt class = "col-sm-2">
            Warranty End Date
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.warrentyEndDate)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Category)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Category)
        </dd>
        <dt class = "col-sm-2">
            Lifespan
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.lifeSpan)
        </dd>
        <dt class = "col-sm-2">
            Uploaded by
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.UserDevice.Name)
        </dd>
    </dl>
</div>

